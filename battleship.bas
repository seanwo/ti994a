2850 REM SUBROUTINE ***VALIDATESHIP***
2860 SUB VALIDATESHIP(SHIPERR,PLAYER,CURRENTSHIP,SHIP(),SHIPS(,,))
2870 SHIPERR=0
2880 HORIZONTAL=0
2890 CALL GETSHIPLEN(SHIPLEN,(CURRENTSHIP))
2900 CALL CHECKHORIZONTAL(HORIZONTAL,(CURRENTSHIP),SHIP())
2910 DIM SEQUENCE(4)
2920 FOR I=0 TO SHIPLEN-1
2930 COL=SHIP(I)-INT(SHIP(I)/16)*16 :: ROW=INT(SHIP(I)/16)
