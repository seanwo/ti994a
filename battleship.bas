2200 REM SUBROUTINE ***COMPUTERDEPLOY***
2210 SUB COMPUTERDEPLOY(VISIBLE,SHIPS(,,))
2220 DIM SHIP(4)
2230 CALL GETNUMSHIPS(NUMSHIPS)
2240 CALL GETBOARDORIG(ROW,COL)
2250 CALL GETSHIPCHAR(SHIPCHAR)
2260 FOR CURRENTSHIP=0 TO NUMSHIPS-1
2270 REM LABEL ***COMPUTERDEPLOYSHIP***
2280 CALL GETSHIPLEN(SHIPLEN,(CURRENTSHIP))
2290 HORIZONTAL=INT(RND*2)
2300 SHIPBEGIN=INT(RND*(10-SHIPLEN))+1
2310 STATIC=INT(RND*10)+1
2320 FOR I=0 TO SHIPLEN-1
2330 IF HORIZONTAL=1 THEN SHIP(I)=16*STATIC+SHIPBEGIN+I ELSE SHIP(I)=16*(SHIPBEGIN+I)+STATIC
2340 NEXT I
2350 PLAYER=1
2360 SHIPERR=0
2370 CALL CHECKOVERLAP(SHIPERR,(PLAYER),(CURRENTSHIP),SHIP(),SHIPS(,,))
2380 IF SHIPERR=1 THEN 2270 :: REM GOTO COMPUTERDEPLOYSHIP
2390 FOR I=0 TO SHIPLEN-1
2400 SHIPS(1,CURRENTSHIP,I)=SHIP(I)
2410 NEXT I
2420 CALL GETDEBUGFLAG(DEBUG)
2430 IF DEBUG=1 THEN VISIBLE=1
2440 IF VISIBLE=1 THEN CALL RENDERSHIP((CURRENTSHIP),SHIP())
2450 NEXT CURRENTSHIP
2460 SUBEND
