1860 ACCEPT AT(ROW+4,COL+4)VALIDATE("YN")BEEP SIZE(1):AUTODEPLOY$
1870 IF AUTODEPLOY$="N" THEN 1990 :: REM GOTO PLAYERDEPLOYSHIPS
1880 CALL RENDERTEXT("AUTO",(ROW+6),(COL))
1890 CALL RENDERTEXT("DEPLOYING",(ROW+7),(COL))
