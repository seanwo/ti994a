590 REM SUBROUTINE ***PLAYGAME***
600 SUB PLAYGAME(SHIPS(,,))
610 CALL CLEARMENU
620 CALL TARGETMENU
630 DIM SHOTS(1,9,9) :: REM SHOTS[PLAYER, ROW,COL]
640 DIM HITS(1,4) :: REM HITS[PLAYER,SHIP]
650 WINNER=0
660 REM LABEL ***FIRELOOP***
670 CALL INPUTTARGET(ROW$,COL)
680 ROW=ASC(ROW$)-64
690 LOC=16*ROW+COL
700 PLAYER=0 :: ERRORVAL=0
710 CALL VALIDSHOT(ERRORVAL,(ROW),(COL),(PLAYER),SHOTS(,,))
720 IF ERRORVAL=1 THEN 660 :: REM GOTO FIRELOOP
730 SHOTS(PLAYER,ROW-1,COL-1)=1
740 PLAYER=1 :: HIT=0 :: SHIP=0
750 CALL CHECKHIT(HIT,SHIP,(LOC),(PLAYER),SHIPS(,,))
760 PLAYER=0
770 CALL RENDERSHOT((PLAYER),(HIT),(ROW),(COL))
780 IF HIT=0 THEN 820 :: REM GOTO ENDTURN
790 PLAYER=1
800 HITS(PLAYER,SHIP)=HITS(PLAYER,SHIP)+1
810 REM ***DETECT SUNK SHIP***
820 REM ***TBD***
830 GOTO 660 :: REM GOTO FIRELOOP
840 SUBEND
850 REM SUBROUTINE ***RENDERSHOT***
860 SUB RENDERSHOT(PLAYER,HIT,SROW,SCOL)
870 CALL GETMENUORIG(ROW,COL)
880 IF PLAYER=0 THEN ROW=ROW+4 ELSE ROW=ROW+9
890 IF HIT=0 THEN TEXT$="(MISS)" :: NOISE=-3 ELSE TEXT$="(HIT)" :: NOISE=-8
900 CALL HCHAR(ROW,COL,32,10)
910 CALL RENDERTEXT((TEXT$),(ROW),(COL))
920 IF HIT=1 THEN CALL GETHITCHAR(CHARVAL) ELSE CALL GETMISSCHAR(CHARVAL)
930 IF PLAYER=1 THEN CALL GETAUXORIG(ROW,COL) ELSE CALL GETBOARDORIG(ROW,COL)
940 TROW=SROW-1 :: TCOL=SCOL-1
950 IF PLAYER=0 THEN TROW=TROW*2 :: TCOL=TCOL*2
960 CALL HCHAR(ROW+TROW,COL+TCOL,CHARVAL,1)
970 CALL SOUND(500,NOISE,2)
980 SUBEND
990 REM SUBROUTINE ***VALIDSHOT***
