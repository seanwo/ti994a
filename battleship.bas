3390 REM SUBROUTINE ***CHECKOVERLAP***
3400 SUB CHECKOVERLAP(SHIPERR,PLAYER,CURRENTSHIP,SHIP(),SHIPS(,,))
3410 SHIPERR=0
3420 CALL GETSHIPLEN(CURRENTSHIPLEN,(CURRENTSHIP))
3430 FOR I=0 TO CURRENTSHIP-1
3440 CALL GETSHIPLEN(SHIPLEN,(I))
3450 FOR J=0 TO SHIPLEN-1
3460 STOREDLOC=SHIPS(PLAYER,I,J)
3470 FOR K=0 TO CURRENTSHIPLEN-1
3480 LOC=SHIP(K)
3490 IF LOC=STOREDLOC THEN SHIPERR=1 :: GOTO 3530 :: REM SUBEND
3500 NEXT K
3510 NEXT J
3520 NEXT I
3530 SUBEND
3540 REM SUBROUTINE ***CHECKHORIZONTAL***
3550 SUB CHECKHORIZONTAL(HORIZONTAL,CURRENTSHIP,SHIP())
3560 HORIZONTAL=0
3570 CALL GETSHIPLEN(SHIPLEN,(CURRENTSHIP))
3580 FOR I=1 TO SHIPLEN-1
3590 PREV=INT(SHIP(I-1)/16) :: CURR=INT(SHIP(I)/16)
3600 IF PREV<>CURR THEN 3630 :: REM SUBEND
3610 NEXT I
3620 HORIZONTAL=1
3630 SUBEND
3640 REM SUBROUTINE ***CHECKSEQUENCE***
3650 SUB CHECKSEQUENCE(SHIPERR,CURRENTSHIP,SEQUENCE())
3660 SHIPERR=0
3670 CALL GETSHIPLEN(SHIPLEN,(CURRENTSHIP))
3680 MINCOORD=SEQUENCE(0)
3690 FOR I=1 TO SHIPLEN-1
3700 MINCOORD=MIN(MINCOORD,SEQUENCE(I))
3710 NEXT I
3720 EXPSUM=MINCOORD
3730 FOR I=1 TO SHIPLEN-1
3740 EXPSUM=EXPSUM+MINCOORD+I
3750 NEXT I
3760 ACTSUM=SEQUENCE(0)
3770 FOR I=1 TO SHIPLEN-1
3780 ACTSUM=ACTSUM+SEQUENCE(I)
3790 NEXT I
3800 IF EXPSUM<>ACTSUM THEN SHIPERR=1
3810 SUBEND
