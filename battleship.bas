2950 NEXT I
2960 CALL CHECKSEQUENCE(SHIPERR,(CURRENTSHIP),SEQUENCE())
2970 IF SHIPERR=1 THEN 2990 :: REM SUBEND
2980 CALL CHECKOVERLAP(SHIPERR,(PLAYER),(CURRENTSHIP),SHIP(),SHIPS(,,))
2990 SUBEND
